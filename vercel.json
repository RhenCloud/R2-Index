{
    "version": 2,
    "devCommand": "python ./app.py",
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python",
            "config": {
                "pythonVersion": "3.11"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}